<div class="w3-container" style="height: 30vh;">
    <div class="w3-card-4">
        <div class="table-responsive w3-padding">                 
            <table id="data-table" class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>Tag ID</th>
                    <th>Tag Text</th>
                    <th>Datetime Scanned</th>
                    <th>Location Scanned</th>
                    <th>Mode</th>
                    <th>User Uploaded</th>
                    <th>Datetime Upload</th>
                    <th>Status</th>
                  </tr>
                </thead>
                <tbody>
                  <% data.forEach(row => { %>
                    <tr>
                      <td><%= row.id %></td>
                      <td><%= row.tag_id %></td>
                      <td><%= row.tag_text %></td>
                      <td><%= row.datetime_scan %></td>
                      <td><%= row.location_scan%></td>
                      <td><%= row.mode %></td>
                      <td><%= row.user_scan_name %></td>
                      <td><%= row.datetime_upload %></td>
                      <td><%= row.status%></td>
                    </tr>
                  <% }); %>
                </tbody>
              </table>
            
        </div>
    </div>
        
</div>

<script>
